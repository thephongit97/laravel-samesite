{
    "name": "thephongit97/laravel-samesite-clients",
    "description": "Some user agents are known to be incompatible with the `SameSite=None` attribute. This middleware will try to determine and overwrite the Cookie value.",
    "keywords": ["laravel", "cookies", "samesite", "samesite cookie","safari"],
    "require": {
        "php": ">=7.1",
        "laravel/framework": ">=5.0",
        "skorp/detect-incompatible-samesite-useragents": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kadir Ã–zdemir"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/thephongit97/development/laravel-samesite-incompatible-clients/issues"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "thephongit97\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "thephongit97\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "thephongit97\\SameSite\\SameSiteIncompatibleClientsProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
